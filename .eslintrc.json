{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "jest": true
    },
    "plugins": ["react", "jsx-a11y", "import"],
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [
        "error",
        {
            "extensions": [".js", ".jsx"]
        }
        ],
        "global-require": "off",
        "import/prefer-default-export": "off",
        "no-console": "off",
        "no-param-reassign": "off",
        "no-unused-expressions": ["error", { "allowTaggedTemplates": true }]
    }
}